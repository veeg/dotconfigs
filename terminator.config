[global_config]
[keybindings]
  close_term = <Primary>w
  close_window = <Primary><Shift>aring
  go_down = <Primary>j
  go_left = <Primary>h
  go_next = <Primary><Alt>aring
  go_right = <Primary>l
  go_up = <Primary>k
  new_tab = <Primary>t
  next_tab = <Primary><Shift>l
  prev_tab = <Primary><Shift>h
  split_horiz = <Primary>o
  split_vert = <Primary>i
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_color = "#ffffdd"
    background_image = None
    foreground_color = "#000000"
